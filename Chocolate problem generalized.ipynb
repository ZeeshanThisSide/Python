   "source": [
    "z = input()\n",
    "n = z.split(',')\n",
    "m = int(n[0])\n",
    "c = int(n[1])\n",
    "w = int(n[2])\n",
    "k = int(n[3])\n",
    "\n",
    "choco = m//c\n",
    "wrapper = m//c\n",
    "\n",
    "while wrapper//w != 0:\n",
    "    ex = wrapper//w\n",
    "    extra_choco = ex*k\n",
    "    choco = choco + extra_choco\n",
    "    wrapper = wrapper % w + extra_choco\n",
    "print(choco)"
   ]
